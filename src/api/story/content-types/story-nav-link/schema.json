        "shared.button"
      ]
    },
    "label": {
      "type": "string",
      "required": true
    },
    "href": {
      "type": "string",
      "regex": "https?:\\/\\/"
    },
    "highlightText": {
      "type": "string"
    },
    "order": {
      "type": "integer",
      "required": true
    },
    "isButton": {
      "type": "boolean",
      "default": false,
      "required": true
    }
  }
}
