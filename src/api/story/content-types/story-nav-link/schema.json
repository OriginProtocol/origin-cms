{
  "kind": "collectionType",
  "collectionName": "story_nav_links",
  "info": {
    "singularName": "story-nav-link",
    "pluralName": "story-nav-links",
    "displayName": "Story.xyz - Nav Link",
    "description": ""
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "links": {
      "type": "dynamiczone",
